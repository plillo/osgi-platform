CREATE TABLE IF NOT EXISTS `k_auth_user_md5` (
  `user_id` varchar(32) NOT NULL,
  `username` varchar(64) NOT NULL,
  `password` varchar(32) NOT NULL,
  `password_mdate` int(11) unsigned DEFAULT NULL,
  `email` varchar(255) DEFAULT NULL,
  `mobile` varchar(32) DEFAULT NULL,
  `published` tinyint(1) unsigned NOT NULL DEFAULT '0',
  `last_login_date` int(11) unsigned DEFAULT NULL,
  `last_login_ip` varchar(15) DEFAULT NULL,
  `trusted_email` tinyint(1) unsigned NOT NULL DEFAULT '0',
  `trusted_mobile` tinyint(1) unsigned NOT NULL DEFAULT '0',
  `cauthor` varchar(32) DEFAULT NULL,
  `cdate` int(11) unsigned DEFAULT NULL,
  `mauthor` varchar(32) DEFAULT NULL,
  `mdate` int(11) unsigned DEFAULT NULL,
  `lauthor` varchar(32) DEFAULT NULL,
  `ldate` int(11) unsigned DEFAULT NULL,
  `user_data` blob,
  PRIMARY KEY (`user_id`),
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

