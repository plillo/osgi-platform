  `birth_loc_1` varchar(18) DEFAULT NULL,
  `birth_loc_2` varchar(18) DEFAULT NULL,
  `birth_loc_3` varchar(18) DEFAULT NULL,
  `birth_loc_4` varchar(18) DEFAULT NULL,
  `birth_loc_5` varchar(18) DEFAULT NULL,
  `birth_loc_6` varchar(18) DEFAULT NULL,
  `birth_oth_1` varchar(64) DEFAULT NULL,
  `birth_oth_2` varchar(64) DEFAULT NULL,
  `birth_oth_3` varchar(64) DEFAULT NULL,
  `birth_oth_4` varchar(64) DEFAULT NULL,
  `birth_oth_5` varchar(64) DEFAULT NULL,
  `birth_oth_6` varchar(64) DEFAULT NULL,
  `resid_loc_1` varchar(18) DEFAULT NULL,
  `resid_loc_2` varchar(18) DEFAULT NULL,
  `resid_loc_3` varchar(18) DEFAULT NULL,
  `resid_loc_4` varchar(18) DEFAULT NULL,
  `resid_loc_5` varchar(18) DEFAULT NULL,
  `resid_loc_6` varchar(18) DEFAULT NULL,
  `resid_oth_1` varchar(64) DEFAULT NULL,
  `resid_oth_2` varchar(64) DEFAULT NULL,
  `resid_oth_3` varchar(64) DEFAULT NULL,
  `resid_oth_4` varchar(64) DEFAULT NULL,
  `resid_oth_5` varchar(64) DEFAULT NULL,
